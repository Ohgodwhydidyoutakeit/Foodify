{
    "manifest_version": 2,
    "name": "Foodify",
    "version": "1.0",
    "description": "Allows you to add your rating to your restaurants and displays it when needed",
    "icons": {
        "48": "icons/48.png"
      },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "Foodify.js"
            ]
        }
    ]
}